<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context" xmlns="http://www.springframework.org/schema/beans"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">

	<bean id="commons-topease.swing.menuItem.CodeMenuItem"
		class="com.xplus.commons.topease.impl.ui.menuitem.DefaultInternalFrameMenuItem"
		init-method="init">
		<property name="text" value="Code" />
		<property name="actions">
			<list>
				<ref bean="commons-topease.swing.action.menuItem.CodeMenuItemAction" />
			</list>
		</property>
	</bean>

	<bean id="commons-topease.swing.menuItem.ExitMenuItem"
		class="com.xplus.commons.topease.impl.ui.menuitem.DefaultInternalFrameMenuItem"
		init-method="init">
		<property name="text" value="Exit" />
		<property name="actions">
			<list>
				<ref bean="commons-topease.swing.action.ExitAction" />
			</list>
		</property>
	</bean>

	<bean id="commons-topease.swing.menuItem.ExportExcelMenuItem"
		class="com.xplus.commons.topease.impl.ui.menuitem.DefaultInternalFrameMenuItem"
		init-method="init">
		<property name="text" value="ExportExcel" />
		<property name="actions">
			<list>
				<ref
					bean="commons-topease.swing.action.menuItem.ExportExcelMenuItemAction" />
			</list>
		</property>
	</bean>

	<bean id="commons-topease.swing.menuItem.CleanMavenMenuItem"
		class="com.xplus.commons.topease.impl.ui.menuitem.DefaultInternalFrameMenuItem"
		init-method="init">
		<property name="text" value="Clean Maven" />
		<property name="actions">
			<list>
				<ref
					bean="commons-topease.swing.action.menuItem.CleanMavenMenuItemAction" />
			</list>
		</property>
	</bean>
	
	<bean id="commons-topease.swing.menuItem.ScreenCaptureMenuItem"
		class="com.xplus.commons.topease.impl.ui.menuitem.DefaultInternalFrameMenuItem"
		init-method="init">
		<property name="text" value="ScreenCaptrue" />
		<property name="actions">
			<list>
				<ref
					bean="commons-topease.swing.action.menuItem.ScreenCaptureMenuItemAction" />
			</list>
		</property>
	</bean>

	<bean id="commons-topease.swing.menuItem.AboutMenuItem"
		class="com.xplus.commons.topease.impl.ui.menuitem.DefaultInternalFrameMenuItem"
		init-method="init">
		<property name="text" value="About" />
		<property name="actions">
			<list>
				<ref bean="commons-topease.swing.action.menuItem.AboutMenuItemAction" />
			</list>
		</property>
	</bean>

</beans>